@using CursoUdemy.Models
@model List<UsuarioMV>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td>@Html.LabelFor(model => Model[0].iidusuario)</td>
            <td>@Html.LabelFor(model => Model[0].nombrePersona)</td>
            <td>@Html.LabelFor(model => Model[0].nombreusuario)</td>
            <td>@Html.LabelFor(model => Model[0].nombreRol)</td>
            <td>@Html.LabelFor(model => Model[0].nombreTipoUsuario)</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.iidusuario</td>
                <td>@item.nombrePersona</td>
                <td>@item.nombreusuario</td>
                <td>@item.nombreRol</td>
                <td>@item.nombreTipoUsuario</td>
            </tr>
        }
    </tbody>
</table>